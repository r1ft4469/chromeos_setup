var color_scheme = {
        'base00': '#073642',
        'base01': '#dc322f',
        'base02': '#859900',
        'base03': '#b58900',
        'base04': '#268bd2',
        'base05': '#d33682',
        'base06': '#2aa198',
        'base07': '#eee8d5',
        'base08': '#002b36',
        'base09': '#cb4b16',
        'base0A': '#586e75',
        'base0B': '#657b83',
        'base0C': '#839496',
